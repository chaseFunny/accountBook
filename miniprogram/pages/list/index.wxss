.box {
  padding: 10px;
  color: #fff;
}
.box .nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.box .nav .lf {
  font-weight: bold;
}
.box .nav .rg .iconf {
  font-size: 22px;
  font-weight: 400;
  margin-left: 10px;
}
.box .inOut {
  margin-bottom: 10px;
  border-radius: 8px;
  background-color: pink;
}
.box .inOut .header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.box .inOut .number {
  display: flex;
  flex-direction: column;
}
