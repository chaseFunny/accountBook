/**app.wxss**/
@import './assets/icon/iconfont.wxss';
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.pageMain{
  width: 100vw;
  height: 100vh;
  background: #f5f5f5;
}
.pageWai{
  width: 100vw;
  height: 100vh;
  background: #0076f6;
}
/* 小字体 */
.smallWord {
  font-size: 16px;
  color: rgba(69, 90, 100, 0.6);  
}